# mount point	fstype		device			[device2]

/boot		emmc		/dev/bootimg
/recovery	emmc		/dev/recovery
/system	ext4		/dev/block/mmcblk0p8	flags=display="System";storage;wipeingui;backup=1
/cache	ext4		/dev/block/mmcblk0p9	flags=display="Cache";storage;wipeingui;backup=1
/data		ext4		/dev/block/mmcblk0p10
/sdcard	vfat		/dev/block/mmcblk1	/dev/block/mmcblk1p1
